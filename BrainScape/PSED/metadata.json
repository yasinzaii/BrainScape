{
    "info": "Dataset Specific Settings",
    "isDownloaded": true,
    "isDatasetJsonCreated": false,
    "isPreprocessed": true,
    "includeDataset": true,
    "download": {
        "isDownloadable": true,
        "plugin": "OpenNeuroDownloader",
        "profile": "",
        "source": "s3://openneuro.org/ds004199",
        "include": [
            "CHANGES",
            "README",
            "dataset_description.json",
            "participants.json",
            "participants.tsv",
            "sub-*/anat/*"
        ]
    },
    "mapping": {
        "plugin": "RegexMapper",
        "regex": {
            "subject": "^sub-.*$",
            "session": "",
            "type": "^anat$",
            "modality": {
                "t1w": "^.*T1w.nii.gz$",
                "flair": "^.*FLAIR.nii.gz$"
            }
        },
        "excludeSub": [
            "sub-00018",
            "sub-00120"
        ]
    }
}