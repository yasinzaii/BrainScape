{
    "info": "Dataset Specific Settings",
    "isDownloaded": false,
    "isDatasetJsonCreated": false,
    "isPreprocessed": false,
    "includeDataset": true,
    "download": {
        "isDownloadable": true,
        "plugin": "OpenNeuroDownloader",
        "profile": "",
        "source": "s3://openneuro.org/ds000221",
        "include": [
            "CHANGES",
            "README",
            "dataset_description.json",
            "participants.tsv",
            "sub-*/*.tsv",
            "sub-*/ses-*/anat/*",
            "sub-*/ses-*/*.tsv"
        ]
    },
    "mapping": {
        "plugin": "RegexMapper",
        "regex": {
            "subject": "^sub-.*$",
            "session": "^ses-.*$",
            "type": "^anat$",
            "modality": {
                "t1w": "^.*T1w.nii.gz$",
                "t2w": "^.*T2w.nii.gz$"
            }
        },
        "excludeSub": []
    }
}