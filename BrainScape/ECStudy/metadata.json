                "T1W": [
                    "^.*T1w.nii.gz$",
                    "FILE"
                ]
            }
        },
        "participantId": "participant_id",
        "sessionId": "session_id",
        "age": "age",
        "gender": "sex",
        "handedness": "handedness",
        "weight": "body_weight ",
